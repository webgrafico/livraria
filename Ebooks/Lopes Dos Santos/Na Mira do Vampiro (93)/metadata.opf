        <dc:title>Na Mira do Vampiro</dc:title>
        <dc:creator opf:file-as="Santos, Lopes Dos" opf:role="aut">Lopes Dos Santos</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (6.17.0) [https://calibre-ebook.com]</dc:contributor>
