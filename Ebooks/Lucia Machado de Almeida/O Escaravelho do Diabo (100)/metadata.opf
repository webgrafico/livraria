        <dc:title>O Escaravelho do Diabo</dc:title>
        <dc:creator opf:file-as="Almeida, Lúcia Machado de &amp; Cafiero, Mário" opf:role="aut">Lúcia Machado de Almeida</dc:creator>
        <dc:creator opf:file-as="Almeida, Lúcia Machado de &amp; Cafiero, Mário" opf:role="aut">Mário Cafiero</dc:creator>
